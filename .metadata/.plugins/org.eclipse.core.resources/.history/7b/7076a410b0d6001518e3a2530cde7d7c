public class TestJsonToObj {
  public static void main(String args[]){
	  String msg = "{'type':1,'from':'wangwu','to':['zhangsan','lisi'],'content':'          '}";
	  JSONObject jsStr = JSONObject.fromObject(msg);
	  System.out.println("jsStr = " + jsStr);

  }
}
